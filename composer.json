{
    "name": "spatie/array-functions",
    "description": "Some handy array helpers",
    "keywords": [
        "spatie",
        "array",
        "helper",
        "handy"
    ],
    "homepage": "https://github.com/freekmurze/array-functions",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://murze.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "files": ["src/array_functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
